# Environment configuration
NODE_ENV=development
PORT=3020

# Database configuration
DATABASE_URL=postgres://postgres:postgres@localhost:5432/cms_service

# Payload CMS configuration
PAYLOAD_SECRET=your_secure_secret_at_least_10_chars
PAYLOAD_PUBLIC_SERVER_URL=http://localhost:3020
PAYLOAD_ADMIN_EMAIL=admin@example.com
PAYLOAD_ADMIN_PASSWORD=securepassword

# CORS configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Optional S3 configuration
S3_ENABLED=false
# S3_BUCKET=your-s3-bucket
# S3_REGION=us-east-1
# S3_ACCESS_KEY=your-access-key
# S3_SECRET_KEY=your-secret-key 