{
  "name": "review-service",
  "version": "1.0.0",
  "description": "Review service for e-commerce platform",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate -- -d src/config/database.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/config/database.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/config/database.ts"
  },
  "keywords": [
    "reviews",
    "e-commerce",
    "microservice",
    "fastify",
    "typescript"
  ],
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^10.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^1.9.3",
    "dotenv": "^16.3.1",
    "fastify": "^4.21.0",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.16.0",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.24",
    "zod": "^3.22.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.0",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "msw": "^2.8.7",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "vitest": "^3.1.4"
  }
}
