{
  "name": "cart-service",
  "version": "1.0.0",
  "description": "Cart management service for e-commerce system",
  "main": "src/index.ts",
  "scripts": {
    "start": "node -r module-alias/register dist/index.js",
    "dev": "cross-env NODE_ENV=development ts-node -r module-alias/register src/index.ts",
    "dev:reset": "cross-env NODE_ENV=development RESET_DB=true ts-node -r module-alias/register src/index.ts",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "db:reset": "cross-env NODE_ENV=development RESET_DB=true ts-node scripts/resetDb.ts"
  },
  "_moduleAliases": {
    "@": "src",
    "@config": "src/config",
    "@controllers": "src/controllers",
    "@entities": "src/entities",
    "@middleware": "src/middleware",
    "@routes": "src/routes",
    "@services": "src/services",
    "@utils": "src/utils"
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/swagger": "8.3.1",
    "@fastify/swagger-ui": "1.10.1",
    "@fastify/type-provider-json-schema-to-ts": "^2.2.2",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "fastify-swagger": "5.2.0",
    "fastify-zod": "^1.4.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "pg": "^8.11.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "typeorm": "^0.3.17",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
