{
  "name": "pricing-service",
  "version": "1.0.0",
  "description": "Microservice for managing product pricing and currency conversion",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r module-alias/register dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "lint": "eslint . --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/swagger": "^8.8.0",
    "@fastify/swagger-ui": "^1.9.3",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.23.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "pg": "^8.11.3",
    "pino": "^8.15.1",
    "pino-pretty": "^10.2.0",
    "typeorm": "^0.3.17",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/module-alias": "^2.0.2",
    "@types/node": "^20.5.9",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@controllers": "dist/controllers",
    "@middlewares": "dist/middlewares",
    "@routes": "dist/routes",
    "@services": "dist/services",
    "@utils": "dist/utils",
    "@entities": "dist/entities",
    "@types": "dist/types"
  }
}
