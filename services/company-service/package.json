{
  "name": "company-service",
  "version": "1.0.0",
  "description": "B2B E-commerce Company Management Service",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "lint": "eslint . --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "ts-node -r tsconfig-paths/register src/scripts/run-migrations.ts",
    "migration:generate": "npm run typeorm -- migration:generate -d src/config/dataSource.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/dataSource.ts",
    "db:seed": "ts-node -r tsconfig-paths/register src/scripts/seed-database.ts",
    "db:reset": "ts-node -r tsconfig-paths/register src/scripts/reset-database.ts"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.0",
    "@fastify/helmet": "^12.0.0",
    "@fastify/jwt": "^9.0.0-pre.fv5.2",
    "@fastify/swagger": "^9.0.1",
    "@fastify/swagger-ui": "^5.0.0-pre.fv5.2",
    "dotenv": "^16.0.3",
    "fastify": "^5.0.0",
    "fastify-plugin": "^4.5.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.10.0",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "typeorm": "^0.3.12",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.11",
    "@types/pg": "^8.6.6",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.3",
    "vitest": "^3.1.4"
  }
}
