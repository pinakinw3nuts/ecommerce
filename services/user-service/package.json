{
  "name": "@ecommerce/user-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register src/server.ts",
    "build": "node build.js",
    "start": "node -r module-alias/register dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate -- -d src/config/data-source.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/config/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/config/data-source.ts",
    "lint": "eslint . --ext .ts"
  },
  "_moduleAliases": {
    "@": "dist",
    "@config": "dist/config",
    "@controllers": "dist/controllers",
    "@entities": "dist/entities",
    "@middlewares": "dist/middlewares",
    "@routes": "dist/routes",
    "@services": "dist/services",
    "@utils": "dist/utils"
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/type-provider-typebox": "^3.5.0",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.1",
    "fastify": "^4.25.2",
    "pg": "^8.11.3",
    "pino": "^8.21.0",
    "pino-pretty": "^10.3.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.24",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@vitest/coverage-v8": "^1.2.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
