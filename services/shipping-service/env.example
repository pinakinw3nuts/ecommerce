# Server
NODE_ENV=development
PORT=3008
API_PREFIX=/api/v1

# Logging
LOG_LEVEL=info

# CORS
CORS_ORIGIN=*

# Database
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=shipping_service

# JWT
JWT_SECRET=your-secret-key
JWT_EXPIRES_IN=1d

# External services
PRODUCT_SERVICE_URL=http://localhost:3010
ORDER_SERVICE_URL=http://localhost:3011

# FedEx API credentials
FEDEX_CLIENT_ID=your-fedex-client-id
FEDEX_CLIENT_SECRET=your-fedex-client-secret
FEDEX_ACCOUNT_NUMBER=your-fedex-account-number
FEDEX_METER_NUMBER=your-fedex-meter-number

# UPS API credentials
UPS_CLIENT_ID=your-ups-client-id
UPS_CLIENT_SECRET=your-ups-client-secret
UPS_ACCOUNT_NUMBER=your-ups-account-number

# Default shipping origin address
DEFAULT_ORIGIN_NAME=Warehouse
DEFAULT_ORIGIN_ADDRESS_LINE1=123 Warehouse St
DEFAULT_ORIGIN_CITY=New York
DEFAULT_ORIGIN_STATE=NY
DEFAULT_ORIGIN_POSTAL_CODE=10001
DEFAULT_ORIGIN_COUNTRY=US
DEFAULT_ORIGIN_PHONE=555-555-5555

# Cache configuration
CACHE_TTL=3600

# Rate comparison configuration
DEFAULT_RATE_CRITERIA=price 